{
  "name": "history-extra",
  "version": "5.0.0",
  "description": "Extra functionality for the history module",
  "main": "dist/createHashStateHistory.js",
  "module": "dist/createHashStateHistory.mjs",
  "scripts": {
    "lint": "eslint '*.{js,mjs}' 'src/**/*.{js,mjs}'",
    "precommit": "lint-staged",
    "version": "npm run build && npm-auto-version",
    "build": "rollup -c rollup.build.js",
    "test": "karma start karma.conf.js",
    "coverage-report": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w33ble/history-extra.git"
  },
  "keywords": [],
  "files": [
    "src/",
    "dist/",
    "CHANGELOG.md",
    "AUTHORS.md",
    "LICENSE"
  ],
  "author": "Joe Fleming (https://github.com/w33ble)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w33ble/history-extra/issues"
  },
  "homepage": "https://github.com/w33ble/history-extra",
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,mjs}": [
        "eslint --fix"
      ],
      "*.{js,mjs,json,css}": [
        "prettier --write"
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "history": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@w33ble/npm-auto-tools": "*",
    "codecov": "^3.1.0",
    "dotenv": "^6.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "history": "^4.7.0",
    "husky": "^0.14.3",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-rollup-preprocessor": "^6.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-tap": "^4.1.4",
    "lint-staged": "^7.0.4",
    "prettier": "^1.9.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "tape-rollup": "^4.6.4"
  },
  "dependencies": {}
}
